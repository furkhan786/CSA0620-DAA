#include <stdio.h>

int findMax(int arr[], int n) {
    int max = arr[0];

    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }

    return max;
}

int main() {
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {7, 7, 7, 7, 7};
    int arr3[] = {-10, 2, 3, -4, 5};

    printf("%d\n", findMax(arr1, 5));  // 5
    printf("%d\n", findMax(arr2, 5));  // 7
    printf("%d\n", findMax(arr3, 5));  // 5

    return 0;
}
